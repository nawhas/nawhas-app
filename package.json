    "vue": "^2.5.17"
  },
  "dependencies": {
    "node-vibrant": "^3.0.0",
    "style-loader": "^0.23.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "vue-nprogress": "^0.1.5",
    "vue-progressbar": "^0.7.5",
    "vue-router": "^3.0.1",
    "vuetify": "^1.3.3",
    "vuex": "^3.0.1"
  }
